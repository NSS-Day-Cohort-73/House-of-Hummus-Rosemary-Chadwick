Table entrees {
    id int pk
    name varchar
    price float
}

Table vegetables {
    id int pk
    type varchar
    price float
}

Table sides {
    id int pk
    title varchar
    price float
}

Table purchases {
    id int pk
    entreeId int [ref: > entrees.id]
    vegetableId int [ref: > vegetables.id]
    sideId int [ref: > sides.id]
}